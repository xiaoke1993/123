@article{Delgado2012251,
title = "A Constraint Programming model for fast optimal stowage of container vessel bays ",
journal = "European Journal of Operational Research ",
volume = "220",
number = "1",
pages = "251 - 261",
year = "2012",
note = "",
issn = "0377-2217",
doi = "http://doi.org/10.1016/j.ejor.2012.01.028",
url = "http://www.sciencedirect.com/science/article/pii/S0377221712000483",
author = "Alberto Delgado and Rune Moller Jensen and Kira Janstrup and Trine Hoyer Rose and Kent Hoj Andersen",
keywords = "Container vessel stowage planning",
keywords = "Slot planning",
keywords = "Constraint Programming",
keywords = "Integer Programming ",
abstract = "Container vessel stowage planning is a hard combinatorial optimization problem with both high economic and environmental impact. We have developed an approach that often is able to generate near-optimal plans for large container vessels within a few minutes. It decomposes the problem into a master planning phase that distributes the containers to bay sections and a slot planning phase that assigns containers of each bay section to slots. In this paper, we focus on the slot planning phase of this approach and present a Constraint Programming and Integer Programming model for stowing a set of containers in a single bay section. This so-called slot planning problem is NP-hard and often involves stowing several hundred containers. Using state-of-the-art constraint solvers and modeling techniques, however, we were able to solve 90% of 236 real instances from our industrial collaborator to optimality within 1 second. Thus, somewhat to our surprise, it is possible to solve most of these problems optimally within the time required for practical application. "
}
