@article{Christensen2017151,
title = "A matheuristic for the Cargo Mix Problem with Block Stowage ",
journal = "Transportation Research Part E: Logistics and Transportation Review ",
volume = "97",
number = "",
pages = "151 - 171",
year = "2017",
note = "",
issn = "1366-5545",
doi = "http://doi.org/10.1016/j.tre.2016.10.005",
url = "http://www.sciencedirect.com/science/article/pii/S1366554516303775",
author = "Jonas Christensen and Dario Pacino",
keywords = "Liner shipping",
keywords = "Cargo composition",
keywords = "Maritime logistic",
keywords = "Block stowage",
keywords = "Stowage planning",
keywords = "Matheuristic ",
abstract = "Abstract The cargo-mix problem aims at selecting the amount of containers of a given type to load on a vessel. In this article we present an extended definition that includes the analysis of a circular route with draft restrictions, limitations on expected cargo and the use of a block stowage strategy. A compact formulation of the problem based on the state-of-the-art heuristic decomposition is shown not to be able to solve the extended problem, thus a matheuristic approach is presented that can achieve high quality results in a matter of seconds. "
}
@Article{Brouer2017,
author="Brouer, Berit Dangaard
and Karsten, Christian Vad
and Pisinger, David",
title="Optimization in liner shipping",
journal="4OR",
year="2017",
volume="15",
number="1",
pages="1--35",
abstract="Seaborne trade is the lynchpin in almost every international supply chain, and about 90{\%} of non-bulk cargo worldwide is transported by container. In this survey we give an overview of data-driven optimization problems in liner shipping. Research in liner shipping is motivated by a need for handling still more complex decision problems, based on big data sets and going across several organizational entities. Moreover, liner shipping optimization problems are pushing the limits of optimization methods, creating a new breeding ground for advanced modelling and solution methods. Starting from liner shipping network design, we consider the problem of container routing and speed optimization. Next, we consider empty container repositioning and stowage planning as well as disruption management. In addition, the problem of bunker purchasing is considered in depth. In each section we give a clear problem description, bring an overview of the existing literature, and go in depth with a specific model that somehow is essential for the problem. We conclude the survey by giving an introduction to the public benchmark instances LINER-LIB. Finally, we discuss future challenges and give directions for further research.",
issn="1614-2411",
doi="10.1007/s10288-017-0342-6",
url="http://dx.doi.org/10.1007/s10288-017-0342-6"
}
@article{Ding2015242,
title = "Stowage planning for container ships: A heuristic algorithm to reduce the number of shifts ",
journal = "European Journal of Operational Research ",
volume = "246",
number = "1",
pages = "242 - 249",
year = "2015",
note = "",
issn = "0377-2217",
doi = "http://doi.org/10.1016/j.ejor.2015.03.044",
url = "http://www.sciencedirect.com/science/article/pii/S0377221715002660",
author = "Ding Ding and Mabel C. Chou",
keywords = "Stowage planning",
keywords = "Container movement",
keywords = "Shifts",
keywords = "Heuristic algorithm ",
abstract = "Abstract We consider the stowage planning problem of a container ship, where the ship visits a series of ports sequentially and containers can only be accessed from the top of the stacks. At some ports, certain containers will be unloaded temporarily and will be loaded back later for various purposes. Such unproductive movements of containers are called shifts, which are both time and money consuming. Literature shows that binary linear programming formulation for such problems is impracticable for real life problems due to the large number of binary variables and constraints. Therefore, we develop a heuristic algorithm which can generate stowage plans with a reasonable number of shifts for such problems. The algorithm, verified by extensive computational experimentations, performs better than the Suspensory Heuristic Procedure (SH algorithm) proposed in Avriel et?al. (1998), which, to the best of our knowledge, is one of the leading heuristic algorithms for such stowage planning problem. "
}

@article{Monaco2014256,
title = "The Terminal-Oriented Ship Stowage Planning Problem ",
journal = "European Journal of Operational Research ",
volume = "239",
number = "1",
pages = "256 - 265",
year = "2014",
note = "",
issn = "0377-2217",
doi = "http://doi.org/10.1016/j.ejor.2014.05.030",
url = "http://www.sciencedirect.com/science/article/pii/S0377221714004536",
author = "Maria Flavia Monaco and Marcello Sammarra and Gregorio Sorrentino",
keywords = "Logistics",
keywords = "Optimization models",
keywords = "Metaheuristics",
keywords = "Container terminals ",
abstract = "Abstract The Ship Stowage Planning Problem is the problem of determining the optimal position of containers to be stowed in a containership. In this paper we address the problem considering the objectives of the terminal management that are mainly related to the yard and transport operations. We propose a Binary Integer Program and a two-step heuristic algorithm. An extensive computational experience shows the efficiency and effectiveness of our approach. A classification scheme for stowage planning problems is also provided. "
}
@article{Lehnfeld2014297,
title = "Loading, unloading and premarshalling of stacks in storage areas: Survey and classification ",
journal = "European Journal of Operational Research ",
volume = "239",
number = "2",
pages = "297 - 312",
year = "2014",
note = "",
issn = "0377-2217",
doi = "http://doi.org/10.1016/j.ejor.2014.03.011",
url = "http://www.sciencedirect.com/science/article/pii/S0377221714002252",
author = "Jana Lehnfeld and Sigrid Knust",
keywords = "Loading",
keywords = "Unloading",
keywords = "Premarshalling",
keywords = "Storage",
keywords = "Stack",
keywords = "Classification scheme ",
abstract = "Abstract Problems of loading, unloading and premarshalling of stacks as well as combinations thereof appear in several practical applications, e.g. container terminals, container ship stowage planning, tram depots or steel industry. Although these problems seem to be different at first sight, they hold plenty of similarities. To precisely unite all aspects, we suggest a classification scheme and show how problems existing in literature can be described with it. Furthermore, we give an overview of known complexity results and solution approaches. "
}

